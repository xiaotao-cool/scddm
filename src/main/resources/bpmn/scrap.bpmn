<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions
        xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        targetNamespace="http://scddm/process">

    <bpmn:process id="scrapProcess" name="设备报废审批流程" isExecutable="true">

        <bpmn:startEvent id="start" name="提交报废申请"/>

        <bpmn:userTask id="approveTask" name="管理员审批"/>

        <bpmn:endEvent id="end" name="审批完成"/>

        <bpmn:sequenceFlow id="flow1"
                           sourceRef="start"
                           targetRef="approveTask"/>

        <bpmn:sequenceFlow id="flow2"
                           sourceRef="approveTask"
                           targetRef="end"/>

    </bpmn:process>
</bpmn:definitions>
